generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Shopping {
  id         Int      @id @default(autoincrement())
  productId  Int
  name       String
  price      Float
  quantity   Int
  createdAt  DateTime @default(now())
}


